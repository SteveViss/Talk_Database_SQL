\definecolor{foreground}{RGB}{105,104,101}
\definecolor{text}{RGB}{25,24,21}
\definecolor{uqarblue}{RGB}{40,102,151}
\definecolor{uqarred}{RGB}{213,26,35}
\definecolor{uqargreen}{RGB}{100,133,33}
\definecolor{uqargreen2}{RGB}{150,189,74}
\definecolor{uqarorange}{RGB}{237,190,0}
\definecolor{BlueCust}{RGB}{0,141,222}

\setbeamercolor{normal text}{fg=text}
\setbeamercolor{frametitle}{fg=BlueCust}
\setbeamercolor{framesubtitle}{fg=text}

\setbeamercolor{title}{fg=BlueCust}
\setbeamercolor{subtitle}{fg=black}

\setbeamercolor{alerted text}{fg=BlueCust}
\setbeamercolor{structure}{fg=black}

\setbeamercolor{footlinecolor}{fg=black}
\setbeamercolor{section in head/foot}{fg=BlueCust}

%%%%%%%%%%% Block color sets %%%%%%%%%%%%%%%%

% Standard block
\setbeamercolor{block title}{fg = BlueCust, bg = BlueCust}
\setbeamercolor{block body}{bg = black}

% Alert block
\setbeamercolor{block title alerted}{fg = uqarred, bg = uqarred!30}
\setbeamercolor{block body alerted}{ bg = uqarred!30}

% Example block
\setbeamercolor{block title example}{fg = uqargreen, bg = uqargreen!30}
\setbeamercolor{block body example}{ bg = uqargreen!30}

